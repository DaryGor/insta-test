{
    "extends": ["eslint:recommended"],
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "parserOptions": { "ecmaVersion": 6 },
    "rules": {
        "eqeqeq": "error",
        "quotes": ["warn", "single"],
        "no-console": "off",
        "array-element-newline": "off",
        "indent": ["error", 4],
        "newline-per-chained-call": ["warn", { "ignoreChainWithDepth": 2 }]
    }
}
